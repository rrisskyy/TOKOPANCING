          <div class="col-lg-4 col-md-3 mt-4 best-items">
            <div class="card scaled-card">
              <div class="crown-position"><i class="fas fa-crown position-absolute crown text-primary"></i></div>
              <div class="card-container">
                <div class="top text-center">
                  <img src="<%= '/uploads/images/' + items[i].fileName %> " class="imgBig" alt="">
                </div>
                <div class="bottom">
                  <div class="details">
                    <h2><%= items[i].name %></h2>
                    <hr>
                    <p>Rp. <%= items[i].price %></p>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <% }; %> 
