<!-- Slider main container -->
<section class="container">
  <div class="swiper container-left">
    <!-- Additional required wrapper -->
    
    <h2 class="text-primary poppins"><%= category[1] %> </h2>
    <div class="swiper-wrapper">
      <!-- Slides -->
      <% itemsCategories[1].forEach((item) => { %> 
        <div class="swiper-slide">
          <div class="card">
            <div class="card-container">
              <div class="top text-center">
                <img src="<%= '/uploads/images/' + item.fileName %>" class="imgBig" alt="">
              </div>
              <div class="bottom">
                <div class="details">
                  <h2><%= item.name %></span></h2>
                  <hr>
                  <p>Rp. <%= item.price %></p>
                </div>
              </div>
            </div>
          </div>
        </div>
      <% }) %>
      <!-- End Slides -->

    </div>
    <!-- If we need pagination -->
  <div class="swiper-pagination"></div>
  </div>
</section>









